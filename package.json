{
  "name": "express-tools",
  "version": "0.0.0",
  "description": "It's an abstract minimalist express tools, fast, and smoothly <()()~);",
  "main": "app.ts",
  "scripts": {
    "on": "npx nodemon app.ts",
    "p-studio": "prisma studio --schema ./src/Databases/prisma/schema.prisma",
    "p-migrate": "prisma migrate dev --schema ./src/Databases/prisma/schema.prisma",
    "p-migrate-deploy": "prisma migrate deploy --schema ./src/Databases/prisma/schema.prisma",
    "p-introspect": "prisma introspect  --schema ./src/Databases/prisma/schema.prisma",
    "p-generate": "prisma generate --schema ./src/Databases/prisma/schema.prisma",
    "p-db-pull": "prisma db pull --schema ./src/Databases/prisma/schema.prisma"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.17",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "nocache": "^3.0.4",
    "node": "^19.3.0",
    "nodemon": "^2.0.20",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "prisma": "^4.8.0"
  }
}
